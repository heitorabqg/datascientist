      ],
      "source": [
        "\n",
        "\n",
        "# ==========================================================\n",
        "# Funções auxiliares\n",
        "# ==========================================================\n",
        "\n",
        "def sigmoid(v):\n",
        "    return 1 / (1 + np.exp(-v))\n",
        "\n",
        "def mse_loss(y, d):\n",
        "    return np.mean((d - y)**2)\n",
        "\n",
        "def cross_entropy_loss(y, d):\n",
        "    # Evita log(0)\n",
        "    y = np.clip(y, 1e-10, 1 - 1e-10)\n",
        "    return -np.mean(d * np.log(y) + (1 - d) * np.log(1 - y))\n",
        "\n",
        "def logistic_regression_train(x, d, eta, Ne, cost_function='cross_entropy'):\n",
        "    \"\"\"\n",
        "    Treina um modelo de regressão logística.\n",
        "    cost_function: 'mse' ou 'cross_entropy'\n",
        "    \"\"\"\n",
        "    Nt, M = x.shape\n",
        "    X = np.hstack((np.ones((Nt, 1)), x))\n",
        "    W = np.random.uniform(-1, 1, (M + 1, 1))\n",
        "\n",
        "    for _ in range(Ne):\n",
        "        v = X @ W\n",
        "        y = sigmoid(v)\n",
        "\n",
        "        if cost_function == 'mse':\n",
        "            e = d - y\n",
        "            grad = -(eta / Nt) * (X.T @ (e * y * (1 - y)))\n",
        "        else:  # cross-entropy\n",
        "            e = d - y\n",
        "            grad = -(eta / Nt) * (X.T @ e)\n",
        "\n",
        "        W = W - grad\n",
        "    return W\n",
        "\n",
        "def predict(x, W):\n",
        "    X = np.hstack((np.ones((x.shape[0], 1)), x))\n",
        "    return sigmoid(X @ W)\n",
        "\n",
        "# ==========================================================\n",
        "# Função para plotar resultados\n",
        "# ==========================================================\n",
        "\n",
        "def plot_decision_boundary(x, d, W, title):\n",
        "    plt.figure(figsize=(6,5))\n",
        "    plt.scatter(x[d[:,0]==0, 0], x[d[:,0]==0, 1], c='red', label='Classe 0')\n",
        "    plt.scatter(x[d[:,0]==1, 0], x[d[:,0]==1, 1], c='blue', label='Classe 1')\n",
        "\n",
        "    # Fronteira de decisão\n",
        "    x1 = np.linspace(-0.2, 1.2, 100)\n",
        "    w0, w1, w2 = W.flatten()\n",
        "    x2 = -(w0 + w1*x1)/w2\n",
        "    plt.plot(x1, x2, 'k--', label='Fronteira de decisão')\n",
        "\n",
        "    plt.title(title)\n",
        "    plt.xlabel('$x_1$')\n",
        "    plt.ylabel('$x_2$')\n",
        "    plt.xlim(-0.2, 1.2)\n",
        "    plt.ylim(-0.2, 1.2)\n",
        "    plt.legend()\n",
        "    plt.grid(True)\n",
        "    plt.show()\n",
        "\n",
        "# ==========================================================\n",
        "# Geração de dados (funções lógicas)\n",
        "# ==========================================================\n",
        "\n",
        "def generate_logic_data(logic_gate):\n",
        "    x = np.array([[0,0], [0,1], [1,0], [1,1]])\n",
        "    if logic_gate == 'AND':\n",
        "        d = np.logical_and(x[:,0], x[:,1]).astype(int).reshape(-1,1)\n",
        "    elif logic_gate == 'OR':\n",
        "        d = np.logical_or(x[:,0], x[:,1]).astype(int).reshape(-1,1)\n",
        "    elif logic_gate == 'XOR':\n",
        "        d = np.logical_xor(x[:,0], x[:,1]).astype(int).reshape(-1,1)\n",
        "    return x, d\n",
        "\n",
        "# ==========================================================\n",
        "# Parâmetros de treinamento\n",
        "# ==========================================================\n",
        "\n",
        "eta = 0.5\n",
        "Ne = 10000  # número de épocas\n",
        "\n",
        "# ==========================================================\n",
        "# Execução para cada porta lógica e função custo\n",
        "# ==========================================================\n",
        "\n",
        "for gate in ['AND', 'OR', 'XOR']:\n",
        "    x, d = generate_logic_data(gate)\n",
        "\n",
        "    # -------- MSE --------\n",
        "    W_mse = logistic_regression_train(x, d, eta, Ne, cost_function='mse')\n",
        "    y_mse = predict(x, W_mse)\n",
        "    y_mse_class = (y_mse >= 0.5).astype(int)\n",
        "    acc_mse = np.mean(y_mse_class == d)\n",
        "\n",
        "    print(f\"\\n=== {gate} (Custo: MSE) ===\")\n",
        "    print(\"Pesos finais:\", W_mse.flatten())\n",
        "    print(f\"Acurácia: {acc_mse*100:.2f}%\")\n",
        "    plot_decision_boundary(x, d, W_mse, f\"{gate} - Regressão Logística (MSE)\")\n",
        "\n",
        "    # -------- ENTROPIA CRUZADA --------\n",
        "    W_ce = logistic_regression_train(x, d, eta, Ne, cost_function='cross_entropy')\n",
        "    y_ce = predict(x, W_ce)\n",
        "    y_ce_class = (y_ce >= 0.5).astype(int)\n",
        "    acc_ce = np.mean(y_ce_class == d)\n",
        "\n",
        "    print(f\"\\n=== {gate} (Custo: Entropia Cruzada) ===\")\n",
        "    print(\"Pesos finais:\", W_ce.flatten())\n",
        "    print(f\"Acurácia: {acc_ce*100:.2f}%\")\n",
        "    plot_decision_boundary(x, d, W_ce, f\"{gate} - Regressão Logística (Entropia Cruzada)\")\n"
      ]
    }
  ],
